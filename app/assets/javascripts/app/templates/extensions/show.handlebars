<td>{{extension.id}}</td>
<td class="data">
  {{#if isEditing}}
    {{view ZenlabsBeta.EditExtensionView}}
  {{else}}
    {{extension.name}}{{extension.notes}}
  {{/if}}
  <div class="commands">
    {{#unless isEditing}}
      <a href="#" {{action "showEdit"}}>Edit</a>
      <a href="#" {{action "destroyRecord"}}>Remove</a>
    {{/unless}}
  </div>
</td>
