{{view view.sidenavView}}
<div id="appscontainer" class="container-fluid">
  <div class="row-fluid">
    <div class="span3">
      <h2>Extensions</h2>
      <ul id="widgetlist">
        {{#each model}}
        <li {{bindAttr class="category :item"}}>
          <span class="appname">{{#linkTo 'extension' this}}{{name}}{{/linkTo}}</span>
        </li>
        {{/each}}
      </div>
    </div>
    <div class="span9">
      {{outlet}}
    </div>
  </div>
</div>  